{
  "name": "multi-agent-feature-dev",
  "version": "1.0.0",
  "description": "Multi-agent system for automated feature development",
  "main": "src/orchestrator.ts",
  "scripts": {
    "dev": "tsx src/run-feature.ts",
    "interactive": "tsx src/interactive.ts",
    "ui": "tsx src/interactive.ts",
    "web": "tsx src/api/server.ts",
    "web:dev": "API_PORT=3456 tsx watch src/api/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/run-feature.js",
    "test": "tsx src/test-runner.ts",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "tsx src/test-runner.ts --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:api": "jest tests/integration/api.test.ts",
    "test-templates": "tsx src/test-templates.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "validate": "npm run test && npm run lint && npm run type-check"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "openapi-types": "^12.1.3",
    "supertest": "^7.1.4",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.7.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "bin": {
    "run-feature": "./run-feature",
    "multi-agent": "./interactive"
  }
}
