{
  "name": "multi-agent-feature-dev",
  "version": "1.0.0",
  "description": "Multi-agent system for automated feature development",
  "main": "src/orchestrator.ts",
  "scripts": {
    "dev": "tsx src/run-feature.ts",
    "interactive": "tsx src/interactive.ts",
    "ui": "tsx src/interactive.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/run-feature.js",
    "test": "tsx src/test-runner.ts",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "tsx src/test-runner.ts --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test-templates": "tsx src/test-templates.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "validate": "npm run test && npm run lint && npm run type-check"
  },
  "dependencies": {
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "bin": {
    "run-feature": "./run-feature",
    "multi-agent": "./interactive"
  }
}