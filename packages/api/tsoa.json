{
  "entryFile": "src/server.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "spec": {
    "outputDirectory": "dist",
    "specVersion": 3,
    "basePath": "/api",
    "name": "Codettea API",
    "description": "REST API for multi-agent feature development orchestration",
    "version": "1.0.0"
  },
  "routes": {
    "routesDir": "src",
    "basePath": "/api"
  },
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "ES2022",
    "module": "commonjs",
    "strict": true
  }
}